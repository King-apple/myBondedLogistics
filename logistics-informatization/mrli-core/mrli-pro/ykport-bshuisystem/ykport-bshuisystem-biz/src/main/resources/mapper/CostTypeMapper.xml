<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.newweb.mrli.ykport.baoshuisystem.mapper.CostTypeMapper">

  <resultMap id="costTypeMap" type="com.newweb.mrli.ykport.baoshuisystem.entity.CostType">
                  <id property="fId" column="F_ID"/>
                        <result property="costTypeCode" column="COST_TYPE_CODE"/>
                        <result property="costType" column="COST_TYPE"/>
                        <result property="rate" column="RATE"/>
                        <result property="remark" column="REMARK"/>
                        <result property="operNam" column="OPER_NAM"/>
                        <result property="recordTim" column="RECORD_TIM"/>
                        <result property="updateNam" column="UPDATE_NAM"/>
                        <result property="updateTim" column="UPDATE_TIM"/>
                        <result property="delNam" column="DEL_NAM"/>
                        <result property="delTim" column="DEL_TIM"/>
                        <result property="userAgreement" column="USER_AGREEMENT"/>
                        <result property="rateUnit" column="RATE_UNIT"/>
                        <result property="goodsCategory" column="GOODS_CATEGORY"/>
                        <result property="storagePlace" column="STORAGE_PLACE"/>
                        <result property="tradeType" column="TRADE_TYPE"/>
                        <result property="importExport" column="IMPORT_EXPORT"/>
            </resultMap>
	<select id="selectFid" resultType="integer">
		SELECT SEQ_COST_TYPE.NEXTVAL FROM DUAL;
	</select>
</mapper>
