<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.newweb.mrli.ykport.baoshuisystem.mapper.ClientInfoMapper">

  <resultMap id="clientInfoMap" type="com.newweb.mrli.ykport.baoshuisystem.entity.ClientInfo">
                  <id property="fId" column="F_ID"/>
                        <result property="companyCode" column="COMPANY_CODE"/>
                        <result property="company" column="COMPANY"/>
                        <result property="businessLicenseNumber" column="BUSINESS_LICENSE_NUMBER"/>
                        <result property="licensePhotoBack" column="LICENSE_PHOTO_BACK"/>
                        <result property="licensePhotoFront" column="LICENSE_PHOTO_FRONT"/>
                        <result property="legalPerson" column="LEGAL_PERSON"/>
                        <result property="certificateNumber" column="CERTIFICATE_NUMBER"/>
                        <result property="certificatePhotoFront" column="CERTIFICATE_PHOTO_FRONT"/>
                        <result property="certificatePhotoBack" column="CERTIFICATE_PHOTO_BACK"/>
                        <result property="address" column="ADDRESS"/>
                        <result property="companyPhone" column="COMPANY_PHONE"/>
                        <result property="bankAccount" column="BANK_ACCOUNT"/>
                        <result property="openingBank" column="OPENING_BANK"/>
                        <result property="accountName" column="ACCOUNT_NAME"/>
                        <result property="contactName" column="CONTACT_NAME"/>
                        <result property="contactPhone" column="CONTACT_PHONE"/>
                        <result property="contactMail" column="CONTACT_MAIL"/>
                        <result property="remark" column="REMARK"/>
                        <result property="operNam" column="OPER_NAM"/>
                        <result property="recordTim" column="RECORD_TIM"/>
                        <result property="updateNam" column="UPDATE_NAM"/>
                        <result property="updateTim" column="UPDATE_TIM"/>
                        <result property="delNam" column="DEL_NAM"/>
                        <result property="delTim" column="DEL_TIM"/>
            </resultMap>
	<select id="selectFid" resultType="java.lang.Long">
		SELECT SEQ_CLIENT_INFO.NEXTVAL FROM DUAL;
	</select>
</mapper>
