		<if test="oilDataStatisticsVo.outConfirmTim != null and oilDataStatisticsVo.outConfirmTim != ''"> and out_confirm_tim = #{oilDataStatisticsVo.outConfirmTim}</if>
		<if test="oilDataStatisticsVo.outQuantityReal != null"> and out_quantity_real = #{oilDataStatisticsVo.outQuantityReal}</if>
		<if test="oilDataStatisticsVo.outCustomsDeclarationNum != null and oilDataStatisticsVo.outCustomsDeclarationNum != ''"> and out_customs_declaration_num = #{oilDataStatisticsVo.outCustomsDeclarationNum}</if>
		 and del_tim is null
		 order by record_tim desc
	</select>
