	<select id="selectPageData" resultMap="oilDataStatisticsVoMap" parameterType="com.newweb.mrli.ykport.baoshuisystem.vo.OilDataStatisticsVo">
		select * from OIL_DATA_STATISTICS
		where 1=1
		<if test="oilDataStatisticsVo.inAccountNum != null and oilDataStatisticsVo.inAccountNum != ''"> and in_account_num = #{oilDataStatisticsVo.inAccountNum}</if>
		<if test="oilDataStatisticsVo.inOperationUnit != null and oilDataStatisticsVo.inOperationUnit != ''"> and in_operation_unit = #{oilDataStatisticsVo.inOperationUnit}</if>
		<if test="oilDataStatisticsVo.inOperationUnitCod != null and oilDataStatisticsVo.inOperationUnitCod != ''"> and in_operation_unit_cod = #{oilDataStatisticsVo.inOperationUnitCod}</if>
		<if test="oilDataStatisticsVo.inUpdateDte != null and oilDataStatisticsVo.inUpdateDte != ''"> and in_update_dte = #{oilDataStatisticsVo.inUpdateDte}</if>
		<if test="oilDataStatisticsVo.oilKnd != null and oilDataStatisticsVo.oilKnd != ''"> and oil_knd like concat(concat('%',#{oilDataStatisticsVo.oilKnd}),'%') </if>
		<if test="oilDataStatisticsVo.loginAccount != null and oilDataStatisticsVo.loginAccount != ''"> and login_account like concat(concat('%',#{oilDataStatisticsVo.loginAccount}),'%') </if>
		<if test="oilDataStatisticsVo.inAddDteStr != null"> and in_add_dte &gt;= to_date(#{oilDataStatisticsVo.inAddDteStr[0]},'yyyy-MM-dd hh24:mi:ss') and in_add_dte &lt;= to_date(#{oilDataStatisticsVo.inAddDteStr[1]},'yyyy-MM-dd hh24:mi:ss')</if>
		<if test="oilDataStatisticsVo.inConsignNam != null and oilDataStatisticsVo.inConsignNam != ''"> and in_consign_nam like concat(concat('%',#{oilDataStatisticsVo.inConsignNam}),'%') </if>
		<if test="oilDataStatisticsVo.inCargoNam != null and oilDataStatisticsVo.inCargoNam != ''"> and in_cargo_nam like concat(concat('%',#{oilDataStatisticsVo.inCargoNam}),'%') </if>
		<if test="oilDataStatisticsVo.inType != null and oilDataStatisticsVo.inType != ''"> and in_type = #{oilDataStatisticsVo.inType}</if>
		<if test="oilDataStatisticsVo.inBlno != null and oilDataStatisticsVo.inBlno != ''"> and in_blno = #{oilDataStatisticsVo.inBlno}</if>
		<if test="oilDataStatisticsVo.inStorageTank != null and oilDataStatisticsVo.inStorageTank != ''"> and in_storage_tank = #{oilDataStatisticsVo.inStorageTank}</if>
		<if test="oilDataStatisticsVo.inQuantityPlan != null"> and in_qantity_plan = #{oilDataStatisticsVo.inQuantityPlan}</if>
		<if test="oilDataStatisticsVo.inDensityStandard != null and oilDataStatisticsVo.inDensityStandard != ''"> and in_density_standard = #{oilDataStatisticsVo.inDensityStandard}</if>
		<if test="oilDataStatisticsVo.inDensity != null"> and in_density = #{oilDataStatisticsVo.inDensity}</if>
		<if test="oilDataStatisticsVo.inStartTim != null and oilDataStatisticsVo.inStartTim != ''"> and in_start_tim = #{oilDataStatisticsVo.inStartTim}</if>
		<if test="oilDataStatisticsVo.inEndTim != null and oilDataStatisticsVo.inEndTim != ''"> and in_end_tim = #{oilDataStatisticsVo.inEndTim}</if>
		<if test="oilDataStatisticsVo.inConfirmTim != null and oilDataStatisticsVo.inConfirmTim != ''"> and in_confirm_tim = #{oilDataStatisticsVo.inConfirmTim}</if>
		<if test="oilDataStatisticsVo.inQuantityReal != null"> and in_quantity_real = #{oilDataStatisticsVo.inQuantityReal}</if>
		<if test="oilDataStatisticsVo.inCustomsDeclarationNum != null and oilDataStatisticsVo.inCustomsDeclarationNum != ''"> and in_customs_declaration_num = #{oilDataStatisticsVo.inCustomsDeclarationNum}</if>
		<if test="oilDataStatisticsVo.outAccountNum != null and oilDataStatisticsVo.outAccountNum != ''"> and out_account_num = #{oilDataStatisticsVo.outAccountNum}</if>
		<if test="oilDataStatisticsVo.outOperationUnit != null and oilDataStatisticsVo.outOperationUnit != ''"> and out_operation_unit = #{oilDataStatisticsVo.outOperationUnit}</if>
		<if test="oilDataStatisticsVo.outOperationUnitCod != null and oilDataStatisticsVo.outOperationUnitCod != ''"> and out_operation_unit_cod = #{oilDataStatisticsVo.outOperationUnitCod}</if>
		<if test="oilDataStatisticsVo.outUpdateDte != null and oilDataStatisticsVo.outUpdateDte != ''"> and out_update_dte = #{oilDataStatisticsVo.outUpdateDte}</if>
		<if test="oilDataStatisticsVo.outAddDteStr != null"> and out_add_dte &gt;= to_date(#{oilDataStatisticsVo.outAddDteStr[0]},'yyyy-MM-dd hh24:mi:ss') and out_add_dte &lt;= to_date(#{oilDataStatisticsVo.outAddDteStr[1]},'yyyy-MM-dd hh24:mi:ss')</if>
		<if test="oilDataStatisticsVo.outConsignNam != null and oilDataStatisticsVo.outConsignNam != ''"> and out_consign_nam like concat(concat('%',#{oilDataStatisticsVo.outConsignNam}),'%') </if>
