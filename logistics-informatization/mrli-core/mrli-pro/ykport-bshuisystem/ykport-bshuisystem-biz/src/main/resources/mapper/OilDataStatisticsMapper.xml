		<if test="oilDataStatisticsVo.outStorageTank != null and oilDataStatisticsVo.outStorageTank != ''"> and out_storage_tank = #{oilDataStatisticsVo.outStorageTank}</if>
		<if test="oilDataStatisticsVo.outQuantityPlan != null"> and out_qantity_plan = #{oilDataStatisticsVo.outQuantityPlan}</if>
		<if test="oilDataStatisticsVo.outDensityStandard != null and oilDataStatisticsVo.outDensityStandard != ''"> and out_density_standard = #{oilDataStatisticsVo.outDensityStandard}</if>
		<if test="oilDataStatisticsVo.outDensity != null"> and out_density = #{oilDataStatisticsVo.outDensity}</if>
